<script type="text/javascript" 
   src="http://ajax.googleapis.com/ajax/libs/jquery/1.4/jquery.min.js"></script>

<script language="Javascript">
  /*
   * This is a sample jQuert HTTP REST client generated by HTTP4e (http://www.nextinterfaces.com).
   */
  var url = "$httpBean.url";
  var method = "$httpBean.method";
  var params = "$httpBean.paramsForUrl";
  var data = "$httpBean.body";
  var dataType = "text"; //"xml", "html", "script", "json", "jsonp", "text"

  var setRequestHeaders = function(xhr) {
#foreach( $hKey in $httpBean.headers.keySet() )
      xhr.setRequestHeader("$hKey", "$httpBean.headers.get($hKey)");
#end
  }

  var onSuccess = function(responseData, status, xhr) {
      alert("Success");
      debug(xhr);
  };

  var onError = function(xhr) {
      alert("Failure");
      debug(xhr);
  }

  var debug = function(xhr) {		
      if (xhr.readyState == 4) {
      alert("HTTP/1.1 " + xhr.status + " " + xhr.statusText + 
            "\n" + xhr.getAllResponseHeaders() + 
            "\n" + xhr.responseText);	
	}
  }
	
  $httpBean.jquery ({ 
      type: method, 
      url: url, 
      data: data,
      dataType: dataType,
      cache: false, 
      beforeSend: setRequestHeaders,
      success: onSuccess,
      error: onError
  });
</script>